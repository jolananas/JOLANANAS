{
  "name": "@jolananas/frontend",
  "type": "module",
  "version": "3.0.0",
  "description": "JOLANANAS Frontend - React/Next.js Storefront",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:fast": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "postinstall": "prisma generate --schema=app/src/prisma/schema.prisma",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean": "rm -rf .next out dist",
    "build:analyze": "ANALYZE=true next build",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=out",
    "docker:build": "docker build -t jolananas-frontend .",
    "docker:run": "docker run -p 3000:3000 jolananas-frontend",
    "db:generate": "prisma generate --schema=app/src/prisma/schema.prisma",
    "db:push": "prisma db push --schema=app/src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=app/src/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=app/src/prisma/schema.prisma",
    "db:seed": "ts-node --compiler-account ./prisma-seed.ts",
    "db:create-test-user": "tsx scripts/create-test-user.ts",
    "db:cleanup-test-users": "tsx scripts/cleanup-test-users.ts",
    "db:cleanup-test-users:dry-run": "tsx scripts/cleanup-test-users.ts --dry-run"
  },
